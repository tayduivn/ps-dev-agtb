<soda>
	<!-- Leads sub-panel - Edit _Verify that editing "Leads" is correctly canceled on 
		"Leads" sub-panel. -->
	<puts text="Test: Accounts_0260.xml starting." />

	<!--Create a account record -->
	<script file="{@global.scriptsdir}/modules/lib/Account_Create_Lib.xml" />

	<!--Search the record -->
	<link id="moduleTab_Accounts" />
	<div class="moduleTitle">
		<link href="index.php?module=Accounts&amp;action=index" click="false" />
	</div>
	<wait timeout="3" />
	<textfield id="name_basic" set="{@pre_account_name}" />
	<button id="search_form_submit" />
	<table class="list view">
		<link text="{@pre_account_name}" />
	</table>

	<!--Go its detail view -->
	<browser assert="{@pre_account_name}" />

	<!--Create Lead Subpanel -->
	<button id="account_leads_create_button" />
	<wait timeout="5" />
	<csv file="{@global.scriptsdir}/csvs/accounts/Accounts_0044.csv" var="lead_record_0260">
		<form id="form_SubpanelQuickCreate_Leads">
			<textfield id="refered_by" set="{@lead_record_0260.refered_by}" />
			<textfield id="first_name" set="{@lead_record_0260.first_name} {@stamp}" />
			<textfield id="last_name" set="{@lead_record_0260.last_name}" />
			<textfield id="title" set="{@lead_record_0260.title}" />
			<textfield id="phone_work" set="{@lead_record_0260.phone_work}" />
			<textfield id="phone_mobile" set="{@lead_record_0260.phone_mobile}" />
			<textfield id="phone_fax" set="{@lead_record_0260.phone_fax}" />
			<textfield id="department" set="{@lead_record_0260.department}" />

			<select id="lead_source" set="{@lead_record_0260.lead_source}" />
			<select id="status" set="{@lead_record_0260.status}" />
			<checkbox id="do_not_call" set="{@lead_record_0260.do_not_call}" />
		</form>

		<!--user select -->
		<button id="btn_assigned_user_name" />
		<wait timeout="8" />
		<browser assert="User Search" />
		<textfield name="last_name_advanced" set="{@lead_record_0260.assigned_user_name}" />
		<button value="Search" />
		<link text="{@lead_record_0260.assigned_user_name}" />

		<!--teams select -->
		<table id="form_SubpanelQuickCreate_Leads_team_name_table">
			<button value="Select" />
		</table>
		<browser assert="Team Search" />
		<textfield name="name" set="{@lead_record_0260.team_name}" />
		<button value="Search" />
		<link text="{@lead_record_0260.team_name}" />

		<button id="Leads_subpanel_cancel_button" alert="true" />
	</csv>

	<div id="list_subpanel_leads">
		<span class="pageNumbers" assert="(0 - 0 of 0)" />
	</div>

	<!--Delete the record -->
	<script file="{@global.scriptsdir}/modules/lib/Account_Delete_Lib.xml" />
	<puts text="Test: Accounts_0260.xml finished." />
</soda>
