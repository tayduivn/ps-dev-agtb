<soda>
	<!-- Schedule Call_Verify that the call is not scheduled for lead when using "Cancel" 
		function -->
	<puts text="Test: Leads_1939.xml starting" />
	<script file="{@global.scriptsdir}/modules/lib/Lead_Create_Lib.xml" />

	<!--go to the leads -->
	<link id="moduleTab_Leads" />
	<div class="moduleTitle">
		<link href="index.php?module=Leads&amp;action=index" click="false" />
	</div>
	<link text="Basic Search" required="false" />
	<button id="search_form_clear" />
	<textfield id="search_name_basic" set="{@pre_lead_first_name}" />
	<button id="search_form_submit" />
	<link text="{@pre_lead_salutation} {@pre_lead_first_name} {@pre_lead_last_name}" />

	<!--create a call -->
	<div id="list_subpanel_activities">
		<table class="list view">
			<button value="  Log Call  " />
		</table>
	</div>
	<wait timeout="3" />
	<csv file="{@global.scriptsdir}/csvs/leads/Leads_1939.csv" var="Leads_1939">
		<textfield id="name" set="{@Leads_1939.name}" />
		<textfield id="date_start_date" set="{@Leads_1939.date_start_date}" />
		<select id="date_start_hours" set="{@Leads_1939.date_start_hours}" />
		<select id="date_start_minutes" set="{@Leads_1939.date_start_minutes}" />

		<textfield name="duration_hours" set="{@Leads_1939.duration_hours}" />
		<select name="duration_minutes" set="{@Leads_1939.duration_minutes}" />

		<select id="direction" set="{@Leads_1939.direction}" />
		<select id="status" set="{@Leads_1939.status}" />

		<checkbox name="reminder_checked" set="{@Leads_1939.reminder_checked}" />
		<textfield id="description" set="{@Leads_1939.description}" />

		<!--user select -->
		<button id="btn_assigned_user_name" />
		<wait timeout="3" />
		<browser assert="User Search" />
		<textfield name="last_name_advanced" set="{@Leads_1939.assigned_user_name}" />
		<button value="Search" />
		<link text="{@Leads_1939.assigned_user_name}" />

		<!--teams select -->
		<table id="form_SubpanelQuickCreate_Calls_team_name_table">
			<button value="Select" />
		</table>
		<browser assert="Team Search" />
		<textfield name="name" set="{@Leads_1939.team_name}" />
		<button value="Search" />
		<link text="{@Leads_1939.team_name}" />
		<button id="Calls_subpanel_cancel_button" alert="true" />
	</csv>
	<wait timeout="5" />
	<!--verify the call record -->
	<div id="subpanel_activities">
		<span class="pageNumbers" assert="(0 - 0 of 0)" />
	</div>

	<script file="{@global.scriptsdir}/modules/lib/Lead_Delete_Lib.xml" />
	<puts text="Test: Leads_1939.xml finished" />
</soda>
