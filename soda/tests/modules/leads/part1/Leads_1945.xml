<soda>
	<!--
		Compose Email_Verify that the composed email record is not created for lead
		when using "Cancel" function
	-->
	<puts text="Test: Leads_1945.xml starting" />
	<script file="{@global.scriptsdir}/modules/lib/Lead_Create_Lib.xml" />

	<!--go to the leads-->
	<link id="moduleTab_Leads" />
	<browser assert="Leads" />
	<button id="search_form_clear" />
	<textfield id="search_name_basic" set="{@pre_lead_first_name}" />
	<!--<textfield id="search_name_basic" set="{@pre_lead_last_name}"/>-->
	<button id="search_form_submit" />
	<link text="{@pre_lead_salutation} {@pre_lead_first_name} {@pre_lead_last_name}" />

	<!--create a email-->
	<div id="list_subpanel_activities">
		<table class="list view">
			<button value="Compose Email" />
		</table>
	</div>
	<wait timeout="10" />
	<csv file="{@global.scriptsdir}/csvs/leads/Leads_1945.csv" var="Leads_1945">
		<textfield id="emailSubject0" set="{@Leads_1945.emailSubject0}" />
		<div id="container1">
			<link text="Close" click="true" />
		</div>
	</csv>
	<wait timeout="5" />
	<!--verify the email-->
	<div id="list_subpanel_history">
		<span class="pageNumbers" assert="(0 - 0 of 0)" />
	</div>


	<script file="{@global.scriptsdir}/modules/lib/Lead_Delete_Lib.xml" />
	<puts test="Test: Leads_1945.xml finished" />
</soda>
