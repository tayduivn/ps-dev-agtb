<soda>
	<puts text="Test: Opportunity_2311.xml starting" />
	<csv file="{@global.scriptsdir}/csvs/opportunity/Opportunity_2311.csv" var="Opportunity_2311">
		<link id="moduleTab_Opportunities" />
		<browser assert="Opportunities" />
		<button id="search_form_clear" />
		<form id="search_form">
			<textfield id="name_basic" set="{@pre_opp_name}" />
		</form>
		<button id="search_form_submit" />

		<table class="list view">
			<browser assert="{@pre_opp_name}" />
			<link text="{@pre_opp_name}" />
		</table>

		<div id="list_subpanel_contracts">
			<table class="list view">
				<button id="contracts_opportunities_select_button" />
			</table>
		</div>

		<browser assert="Contract Search" />
		<textfield name="name_advanced" set="{@Contract_Create_Lib}" />
		<button value="Search" />
		<table class="list view">
			<link text="{@Contract_Create_Lib}" />
		</table>

		<div id="list_subpanel_contracts">
			<table class="list view">
				<link text="{@Contract_Create_Lib}" click="false" />
				<link class="listViewTdToolsS1" index="2" alert="true" />
				<wait timeout="2" />
				<link text="{@Contract_Create_Lib}" click="false" exist="false" />
			</table>
		</div>
	</csv>
	<puts text="Test: Opportunity_2311.xml finished" />
</soda>
