<soda>
	<puts text="Test: Opportunity_2339.xml starting" />
	<csv file="{@global.scriptsdir}/csvs/opportunity/Opportunity_2339.csv" var="Opportunity_2339">
		<link id="moduleTab_Opportunities" />
		<browser assert="Opportunities" />
		<button id="search_form_clear" />
		<form id="search_form">
			<textfield id="name_basic" set="{@pre_opp_name}" />
		</form>
		<button id="search_form_submit" />

		<table class="list view">
			<browser assert="{@pre_opp_name}" />
			<link text="{@pre_opp_name}" />
		</table>

		<timestamp />

		<div id="list_subpanel_leads">
			<table class="list view">
				<button id="opportunity_leads_create_button" />
			</table>
		</div>

		<wait timeout="2" />

		<!--teams select-->
		<table id="form_SubpanelQuickCreate_Leads_team_name_table">
			<button value="Select" />
		</table>
		<browser assert="Team Search" />
		<textfield name="name" set="{@Opportunity_2339.team_name}" />
		<button value="Search" />
		<link text="{@Opportunity_2339.team_name}" />

		<!--user select-->
		<button id="btn_assigned_user_name" />
		<browser assert="User Search" />
		<textfield name="last_name_advanced" set="{@Opportunity_2339.assigned_user_name}" />
		<button value="Search" />
		<link text="{@Opportunity_2339.assigned_user_name}" />

		<form id="form_SubpanelQuickCreate_Leads">
			<select id="lead_source" set="{@Opportunity_2339.lead_lead_source}" />
			<select id="status" set="{@Opportunity_2339.lead_status}" />
			<textfield id="refered_by" set="{@Opportunity_2339.lead_refered_by}" />

			<textfield name="account_name" set="{@Opportunity_2339.lead_account_name}" />
			<textfield name="first_name" set="{@Opportunity_2339.lead_first_name} {@stamp}" />
			<textfield id="last_name" set="{@Opportunity_2339.lead_last_name}" />
			<!--
				textfield id="title" set="{@Opportunity_2339.lead_title}"/> <textfield
				id="phone_mobile" set="{@Opportunity_2339.lead_phone_mobile}"/> <textfield
				id="phone_fax" set="{@Opportunity_2339.lead_phone_fax}"/> <textfield
				id="department" set="{@Opportunity_2339.lead_department}"/> <textfield
				id="Leads0emailAddress0" set="{@Opportunity_2339.lead_emailAddress0}"/>
				<checkbox id="do_not_call" set="{@Opportunity_2339.lead_do_not_call}"/
			-->
			<button id="Leads_subpanel_cancel_button" alert="true" />
		</form>
		<wait timeout="5" />
		<div id="list_subpanel_leads">
			<table class="list view">
				<browser assert="{@Opportunity_2339.lead_first_name} {@stamp}"
					exist="false" />
			</table>
		</div>
	</csv>
	<puts text="Test: Opportunity_2339.xml finished" />
</soda>
