<soda>
	<puts text="Test: Opportunity_9153.xml starting" />
	<csv file="{@global.scriptsdir}/csvs/opportunity/Opportunity_9153.csv" var="Opportunity_9153">
		<var var="pre_opp_name_a" set="{@pre_opp_name}"/>
		<script file="{@global.scriptsdir}/modules/lib/Opp_Create_Lib_NoDep.xml" />
		<link id="moduleTab_Opportunities" />
		<browser assert="Opportunities" />
		<button id="search_form_clear" />
		<form id="search_form">
			<textfield id="name_basic" set="{@pre_opp_name}" />
		</form>
		<button id="search_form_submit" />

		<form id="MassUpdate">
			<browser assert="{@pre_opp_name}" />
			<checkbox name="massall" class="checkbox" set="false" />
			<link id="actions_link" />
			<link text="Delete" alert="true" message="Please select at least 1 record to proceed." />
			<checkbox name="massall" click="true" />
			<link id="actions_link" />
			<link text="Delete" alert="true" />
			<span class="pageNumbers" assert="(0 - 0 of 0)" />
		</form>
		<var var="pre_opp_name" set="{@pre_opp_name_a}"/>
	</csv>
	<puts text="Test: Opportunity_9153.xml finished" />
</soda>
