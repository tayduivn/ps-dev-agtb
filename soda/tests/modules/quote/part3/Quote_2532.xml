<soda>
   <!-- Edit Call for Quote_Verify that calls related to this Quote can be
      removed successful. 
   -->
    <puts text="Test: Quote_2532.xml started." />
    <csv file="{@global.scriptsdir}/csvs/quote/Quote_2532.csv" var="Quote_2532">
        <link id="moduleTab_Quotes" />
        <browser assert="Quotes" />
        <form id="search_form">
            <button id="search_form_clear" />
            <textfield id="name_basic" set="{@pre_Quote_name}" />
            <button id="search_form_submit" />
        </form>

        <table class="list view">
            <link text="{@pre_Quote_name}" />
        </table>
        
        <div id="list_subpanel_activities">
            <table class="list view">
                <button value="  Log Call  " />
            </table>
        </div>
        <timestamp/>
        <!--teams select-->
        <table id="form_SubpanelQuickCreate_Calls_team_name_table">
            <button value="Select" />
        </table>
        <browser assert="Team Search" />
        <textfield name="name" set="{@Quote_2532.team_name}" /> 
        <button value="Search" />
        <link text="{@Quote_2532.team_name}" />

        <form id="form_SubpanelQuickCreate_Calls">
                <textfield id="name"
                        set="{@Quote_2532.call_name} {@user.name} {@stamp}" />
                <textfield id="date_start_date"
                        set="{@Quote_2532.call_date_start_date}" />
                <select id="date_start_hours"
                        set="{@Quote_2532.call_date_start_hours}" />
                <select id="date_start_minutes"
                        set="{@Quote_2532.call_date_start_minutes}" />
                <textfield name="duration_hours"
                        set="{@Quote_2532.call_duration_hours}" />
                <select name="duration_minutes"
                        set="{@Quote_2532.call_duration_minutes}" />
                <select id="direction" set="{@Quote_2532.call_direction}" />
                <select id="status" set="{@Quote_2532.call_status}" />
                <checkbox name="reminder_checked"
                        set="{@Quote_2532.call_reminder_checked}" />
                <textfield id="description" 
                  set="{@Quote_2532.call_description}" />
                <button value="Save" />
        </form>

        <div id="list_subpanel_activities">
            <link text="{@Quote_2532.call_name} {@user.name} {@stamp}"
                  click="false" />
            <wait timeout="2" />
            <link class="listViewTdToolsS1" index="2" alert="true" />
            <wait timeout="6" />
            <link text="{@Quote_2532.call_name} {@user.name} {@stamp}"
               click="false" exist="false" />
            <span class="pageNumbers" assert="(0 - 0 of 0)" />
        </div>
    </csv>

    <!--Run Delete XML-->
    <script file="{@global.scriptsdir}/modules/lib/Call_Delete_Lib.xml" />
    <puts text="Test: Quote_2532.xml finished." />
</soda>

