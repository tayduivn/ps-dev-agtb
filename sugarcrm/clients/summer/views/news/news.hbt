<div class="widget">

    <div class="news-widget">
        <h2>News</h2>
        {{#each responseData.results}}
        <div class="news-article">
            <h4><a href="{{this.unescapedUrl}}" class="googledoc-fancybox">{{{this.titleNoFormatting}}}</a></h4> ({{{this.publisher}}})
            <p>{{{this.content}}}</p>
        </div>
        {{/each}}
    </div>

</div>