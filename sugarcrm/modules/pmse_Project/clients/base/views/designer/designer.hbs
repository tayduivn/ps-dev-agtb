<style>
    div.designer
    {
        padding-top: 0;
    }
</style>
<link type="text/css" href="modules/pmse_Project/css/jcore.libraries.css" rel="stylesheet" />
<link type="text/css" href="modules/pmse_Project/css/jcore.adam-ui.css" rel="stylesheet" />
<link type="text/css" href="modules/pmse_Project/css/jcore.adam.css" rel="stylesheet" />
<style>
    /* TODO remove this section and put in the sass file */
    #ProjectTitle {
        font-family: Arial,Verdana,Helvetica,sans-serif;
        color: #000000;
        font-weight: bold;
        font-size: 11pt;
        margin: 7px 0 0 0;
    }

    .pmse-toolbar-buttons {
        width: 83%;
        position: absolute;
        left: 0;
    }

    .pmse-toolbar-title {
        width: 17%;
        position: absolute;
        right: 0;
    }
</style>
<!--RECIPES HTML BEGINNIG-->
<div id="container">
    <div class="pane ui-layout-center">
        <div id="jcore_designer" style='background-image: url("modules/pmse_Project/img/grid_20.png");'></div>
    </div>
    <div class="pane ui-layout-east">
        <div id="div-bpmn-error" style="padding: 14px; width: 100%; background-color: #f6f6f6; height: 95%; box-sizing: border-box; overflow:auto"></div>
        <div id = "statusBar" class="adam-error-color" style="height: 5%; width: auto; background: #f6f6f6; position: relative; border-top:1px solid #ddd; box-sizing: border-box; padding: 2px 0 0 5px;">
            <div>
                <i class="adam-status-100-warning icon-exclamation-sign"></i>
                <span id="countErrors" style="position: absolute; left: 28px; padding-top: 2px;"></span>
            </div>
        </div>
        <!--<div class="pane east-center no-border"><div id="adam_tree"></div></div>
        <div class="pane east-south no-border">
            <div id="properties-grid">
            </div>
        </div>-->
    </div>
    <div class="pane ui-layout-north no-border" style="background-color: #CCCCCC;">
        <div id="adam_toolbar">
            <div class="pmse-toolbar-buttons">
                <span id="AdamEventLead" type="draggable" rel="tooltip" title="" data-placement="bottom" data-original-title="{{str "LBL_PMSE_ADAM_DESIGNER_LEADS" this.module}}"><i class="adam-icon-start-lead"></i> </span>
                <span id="AdamEventOpportunity" type="draggable" rel="tooltip" title="" data-placement="bottom" data-original-title="{{str "LBL_PMSE_ADAM_DESIGNER_OPPORTUNITY" this.module}}"><i class="adam-icon-start-opportunity"></i> </span>
                <span id="AdamEventDocument" type="draggable" rel="tooltip" title="" data-placement="bottom" data-original-title="{{str "LBL_PMSE_ADAM_DESIGNER_DOCUMENTS" this.module}}"><i class="adam-icon-start-document"></i> </span>
                <span id="AdamEventOtherModule" type="draggable" rel="tooltip" title="" data-placement="bottom" data-original-title="{{str "LBL_PMSE_ADAM_DESIGNER_OTHER_MODULES" this.module}}"><i class="adam-icon-start-other-module"></i> </span>
                <span class="space"></span>
                <span id="AdamEventTimer" type="draggable" rel="tooltip" title="" data-placement="bottom" data-original-title="{{str "LBL_PMSE_ADAM_DESIGNER_WAIT" this.module}}"><i class="adam-icon-intermediate-timer"></i> </span>
                <span id="AdamEventReceiveMessage" type="draggable" rel="tooltip" title="" data-placement="bottom" data-original-title="{{str "LBL_PMSE_ADAM_DESIGNER_RECEIVE_MESSAGE" this.module}}"><i class="adam-icon-intermediate-receive-message"></i> </span>
                <span id="AdamEventMessage" type="draggable" rel="tooltip" title="" data-placement="bottom" data-original-title="{{str "LBL_PMSE_ADAM_DESIGNER_SEND_MESSAGE" this.module}}"><i class="adam-icon-intermediate-message"></i> </span>
                <span class="space"></span>
                <span id="AdamUserTask" type="draggable" rel="tooltip" title="" data-placement="bottom" data-original-title="{{str "LBL_PMSE_ADAM_DESIGNER_USER_TASK" this.module}}"><i class="adam-icon-user-task"></i> </span>
                <span id="AdamScriptTask" type="draggable" rel="tooltip" title="" data-placement="bottom" data-original-title="{{str "LBL_PMSE_ADAM_DESIGNER_ACTION" this.module}}"><i class="adam-icon-action"></i> </span>
                <span class="space"></span>
                <span id="AdamGatewayExclusive" type="draggable" rel="tooltip" title="" data-placement="bottom" data-original-title="{{str "LBL_PMSE_ADAM_DESIGNER_EXCLUSIVE" this.module}}"><i class="adam-icon-gateway-exclusive"></i> </span>
                <span id="AdamGatewayParallel" type="draggable" rel="tooltip" title="" data-placement="bottom" data-original-title="{{str "LBL_PMSE_ADAM_DESIGNER_PARALLEL" this.module}}"><i class="adam-icon-gateway-parallel"></i> </span>
                <span class="space"></span>
                <span id="AdamEventEnd" type="draggable" rel="tooltip" title="" data-placement="bottom" data-original-title="{{str "LBL_PMSE_ADAM_DESIGNER_END_EVENT" this.module}}"><i class="adam-icon-end"></i> </span>
                <span class="space"></span>
                <span id="AdamTextAnnotation" type="draggable" rel="tooltip" title="" data-placement="bottom" data-original-title="{{str "LBL_PMSE_ADAM_DESIGNER_COMMENT" this.module}}"><i class="adam-icon-textannotation"></i> </span>
                <span class="space"></span>
                <span id="ButtonUndo" type="" rel="tooltip" title="" data-placement="bottom" data-original-title="{{str "LBL_PMSE_ADAM_DESIGNER_UNDO" this.module}}"><i class="adam-icon-undo-off"></i> </span>
                <span id="ButtonRedo" type="" rel="tooltip" title="" data-placement="bottom" data-original-title="{{str "LBL_PMSE_ADAM_DESIGNER_REDO" this.module}}"><i class="adam-icon-redo-off"></i> </span>
                <span id="ButtonSave" type="" rel="tooltip" title="" data-placement="bottom" data-original-title="{{str "LBL_PMSE_ADAM_DESIGNER_SAVE" this.module}}"><i class="adam-icon-save-off"></i> </span>
                <select id="zoom" class="friendly-select">
                    <option value="1">50%</option>
                    <option value="2">75%</option>
                    <option value="3" selected= "selected">100%</option>
                    <option value="4">150%</option>
                    <option value="5">175%</option>
                </select>
            </div>
            <div class="pmse-toolbar-title">
                <div style="display:block; float:right;"><button class="btn btn-invisible btn-close-designer" rel="tooltip" title="" data-placement="bottom" data-original-title="Close Designer">
                    <i class="fa fa-times"></i>
                </button>
                </div>
                <div style="display:block; float:right;">
                    <div id="ProjectTitle"></div>
                    <input id="txt-title" class="input-small" style="margin: 7px 0 0 0; display: none;">
                </div>
                <div class="icon-edit-title" style="display:none; float:right;">
                    <a class="record-edit-link btn btn-invisible" data-type="url" data-name="website"><i class="fa fa-pencil"></i></a>
                </div>
            </div>
        </div>

    </div>
</div>
<!--ADAM HTML ENDING-->

